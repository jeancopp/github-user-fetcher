    "printf": "^0.6.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/yargs": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
